{
  "name": "scalajs-webpack",
  "version": "0.0.0",
  "description": "",
  "config": {
    "prettier_files": "**/*.{json,ts,tsx,js,jsx}"
  },
  "main": "dist/bundle.js",
  "scripts": {
    "build": "npm-run-all build:scalajs build:bundle",
    "build:scalajs": "sbt fastOptJS",
    "build:bundle": "webpack",
    "test": "npm-run-all test:*",
    "test:package": "pkg-ok",
    "test:format": "prettier --check $npm_package_config_prettier_files",
    "test:integration": "jest",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write $npm_package_config_prettier_files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximeKjaer/scalajs-webpack.git"
  },
  "keywords": [],
  "author": "Maxime Kjaer <maxime.kjaer@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaximeKjaer/scalajs-webpack/issues"
  },
  "homepage": "https://github.com/MaximeKjaer/scalajs-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.1.0",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.19.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "loader-utils": "^1.4.0"
  }
}
